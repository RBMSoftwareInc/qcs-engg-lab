{
	"name": "qcs-engg-lab",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "npm run sync-assets && vite dev",
		"build": "npm run sync-assets && npm run generate-redirects && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"sync-assets": "mkdir -p static/assets && cp -r assets/* static/assets/ 2>/dev/null || true",
		"generate-redirects": "tsx scripts/generate-redirects.ts",
		"content-triage": "tsx scripts/content-triage.ts",
		"migrate-wp": "tsx scripts/migrate-wordpress.ts"
	},
	"devDependencies": {
		"@mdx-js/mdx": "^3.1.1",
		"@mdx-js/rollup": "^3.1.1",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^25.0.3",
		"rehype-raw": "^7.0.0",
		"rehype-sanitize": "^6.0.0",
		"remark-gfm": "^4.0.1",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vite": "^7.2.6"
	},
	"dependencies": {
		"@sveltejs/adapter-static": "^3.0.10",
		"@tiptap/core": "^3.13.0",
		"@tiptap/extension-image": "^3.13.0",
		"@tiptap/extension-link": "^3.13.0",
		"@tiptap/extension-placeholder": "^3.13.0",
		"@tiptap/extension-typography": "^3.13.0",
		"@tiptap/starter-kit": "^3.13.0",
		"front-matter": "^4.0.2",
		"markdown-it": "^14.1.0",
		"marked": "^17.0.1",
		"prosemirror-markdown": "^1.13.2"
	}
}
